<% if @users.any? %>
  <ul>  <% @users.each do |user| %>
      <li>
        <%= link_to user_url(user), id: "user_container" do %> 
        <%= render partial: "users/user_info", locals: { user: user } %>
        <% end %>
      </li>
    <% end %>
  </ul>
<% else %>
  <p class="no-users">No users found.</p> <% end %>
